			"yield_day_l":			{"type": "word", "pos":8},
			"yield_day_h":			{"type": "word", "pos":10},
			"yield_sum_l":			{"type": "word", "pos":12},
			"yield_sum_h":			{"type": "word", "pos":14, "comment": "Word Umrechnung falsch?"},
			"yield_sum_m":			{"type": "word", "pos":16}
		},
		"computed_values": {
			"yield_day":			{"type": "script", "script": "yield_day_h * 1000 + yield_day_l"},
			"yield_sum":			{"type": "script", "script":	"yield_sum_m * 1000000  + yield_sum_h * 1000 + yield_sum_l"}
		}
	},

